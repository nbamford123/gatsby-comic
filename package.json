{
  "name": "gatsby-comic",
  "private": true,
  "description": "Site for hosting a webcomic",
  "version": "0.1.0",
  "author": "Nathan Bamford <nathan.bamford@gmail.com>",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "@types/react-select": "^3.0.12",
    "gatsby": "^2.20.12",
    "gatsby-background-image": "^0.10.2",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-mdx": "^1.1.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-ts": "^2.7.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-source-instagram": "^0.7.0",
    "gatsby-transformer-sharp": "^2.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-select": "^3.1.0"
  },
  "devDependencies": {
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,json,md}\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc",
    "validate": "npm-run-all --parallel type-check check-format lint build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
